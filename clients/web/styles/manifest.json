{
  "short_name": "Tapster web app",
  "name": "Tapster2 Web Client",
  "description": "Web application we can drive the Tapster 2 robot by sending to it's server HTTP requests",
  "icons": [
    {
      "src": "pictures/logo-x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "pictures/logo-x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "pictures/logo-x192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "serviceworker": {
    "src": "serviceworker.js",
    "scope": "/",
    "update_via_cache": "none"
  },
  "start_url": "/webapp.html",
  "display": "standalone",
  "orientation": "portrait",
  "dir": "auto",
  "lang": "en"
}
